<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- permissions -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cleanenv"
        tools:targetApi="31">
        <activity
            android:name=".ui.home.order_summery"
            android:theme="@style/Theme.Cleanenv.NoActionBar"
            android:exported="false" />
        <activity
            android:name=".ui.home.newAddress"
            android:theme="@style/Theme.Cleanenv.NoActionBar"
            android:exported="false" />
        <activity
            android:name=".ui.home.addressDetails"
            android:theme="@style/Theme.Cleanenv.NoActionBar"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Cleanenv.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SplashScreenAndLogin.Register"
            android:exported="false"
            android:theme="@style/Theme.Cleanenv.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SplashScreenAndLogin.Login"
            android:exported="false"
            android:theme="@style/Theme.Cleanenv.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SplashScreenAndLogin.SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.Cleanenv.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>